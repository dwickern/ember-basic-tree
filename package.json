{
  "name": "ember-basic-tree",
  "version": "2.0.0",
  "description": "A customizable template-driven tree component",
  "keywords": [
    "ember-addon"
  ],
  "homepage": "https://dwickern.github.io/ember-basic-tree",
  "repository": {
    "type": "git",
